{
    "multipart": [
        {
            "when": {
                "axis_y": "true"
            },
            "apply": {
                "model": "gardentrails:block/general/iron_pergola_y"
            }
        },
        {
            "when": {
                "axis_x": "true",
                "axis_z": "false"
            },
            "apply": {
                "model": "gardentrails:block/general/iron_pergola_x"
            }
        },
        {
            "when": {
                "axis_x": "false",
                "axis_z": "true"
            },
            "apply": {
                "model": "gardentrails:block/general/iron_pergola_x",
                "y": 90
            }
        },
        {
            "when": {
                "axis_x": "true",
                "axis_z": "true"
            },
            "apply": {
                "model": "gardentrails:block/general/iron_pergola_x_z"
            }
        },
        {
            "when": {"age": "0", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_0"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_0", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_0", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_0", "y": 270 }
            ]
        },
        {
            "when": {"age": "1", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_1"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_1", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_1", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_1", "y": 270 }
            ]
        },
        {
            "when": {"age": "2", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_2"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_2", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_2", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_2", "y": 270 }
            ]
        },
        {
            "when": {"age": "3", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_3"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_3", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_3", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_3", "y": 270 }
            ]
        },
        {
            "when": {"age": "4", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_4"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_4", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_4", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_4", "y": 270 }
            ]
        },
        {
            "when": {"age": "5", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_5"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_5", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_5", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_5", "y": 270 }
            ]
        },
        {
            "when": {"age": "6", "axis_y" : "true"},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_y_stage_6"},
                { "model": "gardentrails:block/general/grape/grape_y_stage_6", "y": 90 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_6", "y": 180 },
                { "model": "gardentrails:block/general/grape/grape_y_stage_6", "y": 270 }
            ]
        },
        {
            "when": {"OR": [
                {"age": "0", "axis_x": "true" },
                {"age": "0", "axis_z": "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_0"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "1", "axis_x" : "true" },
                {"age": "1", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_1"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "2", "axis_x" : "true" },
                {"age": "2", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_2"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "3", "axis_x" : "true" },
                {"age": "3", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_3"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "4", "axis_x" : "true" },
                {"age": "4", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_4"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "5", "axis_x" : "true" },
                {"age": "5", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_5"}
            ]
        },
        {
            "when": {"OR": [
                {"age": "6", "axis_x" : "true" },
                {"age": "6", "axis_z" : "true" }
            ]},
            "apply": [
                { "model": "gardentrails:block/general/grape/grape_x_z_stage_6"}
            ]
        }
    ]
}
    